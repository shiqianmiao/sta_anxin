.tit-wrap{
    width:70%;
    height:30px;
    background:#ffa200;
    -moz-transform:skewX(-8deg);
    -o-transform:skewX(-8deg);
    -webkit-transform:skewX(-8deg);
    transform:skewX(-8deg);
    position:relative;
    margin:0 auto;
}
.tit-wrap h2{
    width:83%;
    height:30px;
    text-align:center;
    font-size:16px;
    line-height:30px;
    margin:0 auto;
    background:#ffd323;
    -moz-transform:skewX(-2deg);
    -o-transform:skewX(-2deg);
    -webkit-transform:skewX(-2deg);
    transform:skewX(-2deg);
    position:relative;
    top:-4px;
}
.tit-wrap h2:after{
    display:block;
    content:"";
    height:4px;
    width:95%;
    background:#e22f2f;
    position:absolute;
    left:2.5%;
    bottom:-4px;
}
.tit-wrap h2::first-line{
    -moz-transform:skewX(8deg);
    -o-transform:skewX(8deg);
    -webkit-transform:skewX(8deg);
    transform:skewX(8deg);
}
.tit-wrap:before{
    display: block;
    content: "";
    width: 28px;
    height: 40px;
    background: #e22f2f;
    position: absolute;
    left: -24px;
    top: -10px;
    -moz-transform: rotate(-50deg);
    -o-transform: rotate(-50deg);
    -webkit-transform: rotate(-50deg);
    transform: rotate(-50deg);
}
.tit-wrap:after{
    display: block;
    content: "";
    width: 28px;
    height: 40px;
    background: #e22f2f;
    position: absolute;
    right: -24px;
    top: -1px;
    -moz-transform: rotate(-50deg);
    -o-transform: rotate(-50deg);
    -webkit-transform: rotate(-50deg);
    transform: rotate(-50deg);
}
