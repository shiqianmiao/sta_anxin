/*scroll*/
.winner-wrap{
    width:100%;
    overflow:hidden;
    position:relative;
}
.winner-con{
    height:25px;
    line-height:25px;
    overflow:hidden;
    white-space:nowrap;
    position:absolute;
    top:0;
    -moz-animation: swing 26s infinite linear;
    -o-animation: swing 26s infinite linear;
    -webkit-animation: swing 26s infinite linear;
}
@-webkit-keyframes swing {
    0% {
        left:0;
    }
    100% {
        left:-1326px;
    }
}
