.date-box{
    width: 93.75%;
    margin: 0 auto;
    background-color: #fbf2d0;
    border-radius: 8px;
    padding: 10px 0;
}
.now-date ,.part-box .tit:before{
    background: url(http://sta.ganjistatic1.com/att/project/app/ticket/img/icon.__123123123__.png) no-repeat;
    background-size: 93px auto;
}
.now-date{
    padding-top: 10px;
    height: 78px;
    width: 93px;
    background-position: 0 -17px;
    margin:7px 0 0 10px;
}
.now-date p ,.now-date span{
    color: #e22f2f;
    text-align: center;
}
.now-date p{
    font-weight: bold;
    font-size: 18px;
}
.now-date span{
    font-size: 12px;
    display: block;
    line-height: 20px;
}
.now-date ,.buy-date{
    display: inline-block;
    vertical-align: top;
}
.buy-date{
    width: 170px;
    margin:7px 0 0 10px;
}
.buy-date p{
    margin-top: 6px;
}
.buy-date p span{
    display: block;
    color: #4c4c4c;
    font-weight: bold;
}
.buy-date p .s1{
    font-size: 12px;
    color: #e22f2f;
    font-weight: normal;
    line-height: 12px;
}
.part-box .tit{
    position: relative;
    background-color: #ffd323;
    height: 27px;
    line-height: 27px;
    font-size: 16px;
    color: #e22f2f;
    width: 115px;
    margin: 20px 0 10px;
    text-indent: 30px;
}
.buy-ticket .tit{
    width: 85px;
}
.part-box .tit:before{
    content: "";
    position: absolute;
    top: 5px;
    left: 10px;
    width: 16px;
    height: 16px;
}
.calendar .tit:before{
    background-position: 0 0;
}
.hot-time .tit:before{
    background-position: -18px  0;
}
.buy-ticket .tit:before{
    background-position: -35px 3px;
}
.order-plan .tit:before{
    background-position: -52px 1px;
}
.part-box .tit:after{
    content: "";
    position: absolute;
    right: -18px;
    top: 0;
    width: 27px;
    height: 27px;
    background-color: #e22f2f;
    -moz-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
}
.c-img{
    padding: 0 10px;
}
.c-img img{
    width: 100%;
}
.part-bg{
    margin: 0 10px;
    background-color: #fff;
    border-radius: 8px;
}
.part-bg ul{
    width: 210px;
    margin: 0 auto;
    padding: 10px 0;
}
.part-bg li{
    height: 28px;
}
.part-bg li span{
    display: inline-block;
    background-color: #ffe991;
    height: 20px;
    line-height: 20px;
    width: 60px;
    border-radius: 3px;
    text-indent: 5px;
    margin-right: 10px;

}
.part-bg li em{
    padding:  0 10px;
}
.traffic{
    width: 300px;
    margin: 0 auto;
}
.traffic li{
    border: 1px solid #ffa525;
    border-radius: 5px;
    background-color: #fff;
    padding: 4px 8px;
    width: 281px;
    margin-bottom: 10px;
}
.traffic li a{
