    -ms-background-size: 80px auto;
    background-size: 80px auto;
}
.icon-photo::after,
.icon-edit::after,
.icon-pen::after,
.icon-headphone::after,
.icon-mold::after,
.icon-cart::after,
.icon-business-card::after,
.icon-avatar::after,
.icon-trash::after,
.icon-close::after,
.icon-sandglass::after,
.icon-hot::after,
.icon-tel::after,
.input-search-group::before {
    background: url(http://sta.ganji.com/public/image/mobile/app/loan/icon.png?msc=8667) no-repeat;
