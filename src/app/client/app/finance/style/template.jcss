    -ms-box-orient: horizontal;
    box-orient: horizontal;
    overflow: hidden;
    z-index: 200;
    position: relative;
    background: #fff;
}
.filter-nav li {
    display: block;
    -webkit-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-box-flex: 1;
    -ms-box-flex: 1;
    box-flex: 1;
    position: relative;
    width: 1px;
    max-width: 90px;
    height: 40px;
    font-size: 14px;
    line-height: 40px;
    text-align: center;
    padding: 0 21px 0 5px;
    margin-left: 1px;
    cursor: pointer;
    font-weight: bold;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.filter-nav li::before {
    content: "";
    position: absolute;
    top: 50%;
    left: -1px;
    width: 1px;
    height: 18px;
    background-color: #d9d9d9;
    overflow: hidden;
    margin-top: -9px;
    z-index: 100
}
.filter-nav li:first-child::before {
    display: none;
}
.filter-nav li::after {
    content: "";
    position: absolute;
    top: 50%;
    right: 9px;
    width: 5px;
    height: 5px;
    border-top: solid 1px #5b2;
    border-right: solid 1px #5b2;
    -webkit-transform: rotate(135deg);
    margin-top: -5px;
    overflow: hidden;
}
.filter-nav li.active {
    border-bottom: 2px solid #5b2;
}
.filter-nav li.active::after {
    -webkit-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
    margin-top: -1px;
}
.filter-nav li.more {
    -webkit-box-flex: 0;
    -ms-box-flex: 0;
    box-flex: 0;
    width: 50px;
    max-width: 50px;
    padding: 0;
    margin: 0;
}
.filter-nav li.more::before {
    height: 40px;
    margin-top: -20px;
}
.filter-nav li.more::after {
    display: none;
}
.filter-nav .touch {
    border-radius: 0;
}
.filter-nav a, .filter-nav b {
    display: block;
    width: 100%;
    font-weight: normal;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.filter-nav a {
    display: block;
}
