<section class="sel_date">
    <section class="clearfix">
        <span class="prev_m fl js_pre_month"></span>
        <span class="next_m fr js_next_month"></span>
        <section class="show_mn">
            <input type="text" class="showDate2 year js_year" value="<%= year %>" />
            <span class="ml5">年</span>
            <input type="text" class="showDate2 month js_month" value="<%= month %>"/>
            <span class="ml5">月</span>
        </section>
    </section>
    <section class="mb15"></section>
    <table class="data_table">
        <thead>
            <tr>
                <td align="center" valign="middle">日</td>
                <td align="center" valign="middle">一</td>
                <td align="center" valign="middle">二</td>
                <td align="center" valign="middle">三</td>
                <td align="center" valign="middle">四</td>
                <td align="center" valign="middle">五</td>
                <td align="center" valign="middle">六</td>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</section>